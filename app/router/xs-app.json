{
  "routes": [
    {
      "source": "^/tech-library/(.*)$",
      "target": "$1",
      "localDir": "resources/tech-library",
      "authenticationType": "xsuaa"
    },
      {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "srv-api",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    }
  ]
}
